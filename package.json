{
  "name": "iobroker.teltonika",
  "description": "The adapter reads data from Teltonika routers via MQTT and provides it in ioBroker.",
  "version": "0.0.2",
  "author": "bluefox <dogafox@gmail.com>",
  "homepage": "https://github.com/ioBroker/ioBroker.teltonika",
  "repository": {
    "type": "git",
    "url": "https://github.com/ioBroker/ioBroker.teltonika"
  },
  "keywords": [
    "ioBroker",
    "teltonika",
    "router"
  ],
  "dependencies": {
    "@iobroker/adapter-core": "^3.3.2",
    "mqtt-connection": "^4.1.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^5.0.0",
    "@alcalzone/release-script-plugin-iobroker": "^4.0.0",
    "@alcalzone/release-script-plugin-license": "^4.0.0",
    "@alcalzone/release-script-plugin-manual-review": "^4.0.0",
    "@iobroker/adapter-dev": "^1.5.0",
    "@iobroker/eslint-config": "^2.2.0",
    "@iobroker/legacy-testing": "^2.0.2",
    "@iobroker/testing": "^5.2.2",
    "@iobroker/types": "^7.1.0",
    "mqtt": "^5.14.1",
    "typescript": "^5.9.3"
  },
  "bugs": {
    "url": "https://github.com/ioBroker/ioBroker.teltonika/issues"
  },
  "main": "build/main.js",
  "files": [
    "build/",
    "admin/",
    "img/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
      "prepublishOnly": "npm run build",
      "test:integration": "mocha test/*.js --exit",
      "test": "npm run test:integration",
      "lint": "eslint -c eslint.config.mjs",
      "build": "tsc -p tsconfig.build.json",
      "release": "release-script",
      "release-patch": "release-script patch --yes",
      "release-minor": "release-script minor --yes",
      "release-major": "release-script major --yes",
      "update-packages": "npx -y npm-check-updates"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT"
}
